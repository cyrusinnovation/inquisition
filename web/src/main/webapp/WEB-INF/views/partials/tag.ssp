#import(com.trailmagic.jumper.web.util.SecurityHelper)
<%@ val questionId: String %>
<%@ val questionCreator: String %>
<%@ val tag: String %>

#if (SecurityHelper.isUserCurrentlyAuthenticated(questionCreator))
    <a class="deleteTag" href="/delete" value="/questions/${questionId}/tags/">${tag}</a>
#else
    ${tag}
#end